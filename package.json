    "publish": [
      {
        "provider": "generic",
        "url": "http://xxxxx/"
      }
    ],
    "files": [
      {
        "filter": [
          "**/*",
          "!myNodeTest/*",
          "!myWebChat/*"
        ]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
